<% if current_page.data.description %>
    <meta name="description" content="<%= current_page.data.description %>">
<% end %>
<% if current_page.data.title %>
    <meta property="og:title" content="<%= current_page.data.title %>">
<% end %>
<% if current_page.data.type %>
    <meta property="og:type" content="<%= current_page.data.type %>">
<% end %>
<% if current_page.data.og:image %>
    <meta property="og:url" content="<%= current_page.data.url %>">
<% end %>
<% if current_page.data.image %>
    <meta property="og:image" content="<%= current_page.data.image %>">
<% end %>
<% if current_page.data.description %>
    <meta name="twitter:card" content="summary">
<% end %>